{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "npm install"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": "node index.js",
    "restartPolicyMaxRetries": 5,
    "healthchecks": {
      "enabled": false
    }
  }
}
